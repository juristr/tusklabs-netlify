{
  "name": "store-admin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/store-admin/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/store-admin"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "store-admin:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "store-admin:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "store-admin:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "store-admin:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "store-admin:build:development"
        },
        "production": {
          "buildTarget": "store-admin:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/store-admin"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/store-admin/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "store-admin:build"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "outputs": [],
      "dependsOn": ["build"],
      "options": {
        "command": "netlify deploy --filter store-admin --dir dist/apps/store-admin"
      }
    }
  },
  "tags": []
}
